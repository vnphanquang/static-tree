{
  "name": "static-tree",
  "version": "1.1.0",
  "description": "Zero dependency builder for strongly typed static tree",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint **/*.{ts,js}",
    "format": "prettier --check \"./**/*.{ts,js,json}\"",
    "test": "vitest --ui --coverage",
    "ci:test": "vitest run --coverage",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown -i api/temp -o api/docs",
    "api": "run-s api:extract api:document"
  },
  "keywords": [
    "typescript",
    "typed",
    "map",
    "tree",
    "static",
    "builder",
    "structure"
  ],
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnphanquang/static-tree.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/vnphanquang/static-tree/issues"
  },
  "homepage": "https://github.com/vnphanquang/static-tree/tree/main/packages/static-tree",
  "author": "Quang Phan",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.19.5",
    "@microsoft/api-extractor": "^7.29.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^18.7.3",
    "@vitest/coverage-istanbul": "^0.22.1",
    "@vitest/ui": "^0.22.0",
    "@vnphanquang/eslint-config": "^1.1.0",
    "eslint": "^8.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup": "^2.78.1",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vitest": "^0.22.0"
  }
}
