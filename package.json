{
  "name": "static-tree",
  "version": "0.1.0",
  "description": "Zero dependency builder for strongly typed static tree",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint **/*.{ts,js,cjs}",
    "format": "prettier --check \"./**/*.{ts,js,cjs,json}\"",
    "test": "vitest --coverage",
    "test:ui": "vitest --ui --coverage",
    "test:run": "vitest run --coverage"
  },
  "keywords": [
    "typescript",
    "typed",
    "map",
    "tree",
    "static",
    "builder",
    "structure"
