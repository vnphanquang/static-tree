  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.3",
    "@vitest/coverage-istanbul": "^0.22.1",
    "@vitest/ui": "^0.22.0",
    "@vnphanquang/eslint-config": "^1.1.0",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vitest": "^0.22.0"
  }
}
